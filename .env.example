# Twilio Account Configuration
# Get these values from your Twilio Console: https://console.twilio.com
TWILIO_ACCOUNT_SID=your_account_sid_here
TWILIO_API_KEY=your_api_key_here
TWILIO_API_SECRET=your_api_secret_here
TWILIO_TWIML_APP_SID=your_twiml_app_sid_here

# Caller ID Numbers
TWILIO_PHONE_IL=your_israel_phone_number
TWILIO_PHONE_IL_ALT=your_alternate_israel_number
TWILIO_PHONE_US=your_us_canada_phone_number
TWILIO_PHONE_DEFAULT=your_default_phone_number

# ngrok Configuration
# Get your authtoken from: https://dashboard.ngrok.com/get-started/your-authtoken
NGROK_AUTHTOKEN=your_ngrok_authtoken_here

# Flask Configuration
FLASK_ENV=development
FLASK_DEBUG=True
